<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" xmlns="http://javafx.com/javafx/11"
            fx:controller="controllers.LoginController">
    <AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="199.0" prefWidth="817.0"
                style="-fx-background-color: #FFFFFF;" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                AnchorPane.topAnchor="0.0">
        <HBox alignment="CENTER" prefHeight="200.0" prefWidth="817.0" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <Label text="Авторизация">
                <font>
                    <Font name="Book Antiqua" size="64.0"/>
                </font>
            </Label>
        </HBox>
    </AnchorPane>
    <AnchorPane layoutX="10.0" layoutY="197.0" prefHeight="503.0" prefWidth="817.0" AnchorPane.bottomAnchor="0.0"
                AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="200.0">
        <VBox alignment="CENTER" prefHeight="400.0" prefWidth="817.0" style="-fx-background-color: #FFFFFF;"
              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
            <HBox alignment="CENTER" prefHeight="47.0" prefWidth="583.0">
                <Label prefHeight="45.0" prefWidth="150.0" text="Логин">
                    <font>
                        <Font name="Book Antiqua" size="36.0"/>
                    </font>
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                </Label>
                <TextField fx:id="textFieldLogin">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                    <font>
                        <Font name="Book Antiqua" size="36.0"/>
                    </font>
                </TextField>
            </HBox>
            <HBox alignment="CENTER" prefHeight="48.0" prefWidth="600.0">
                <Label prefHeight="45.0" prefWidth="150.0" text="Пароль">
                    <font>
                        <Font name="Book Antiqua" size="36.0"/>
                    </font>
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                </Label>
                <PasswordField fx:id="textFieldPassword">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                    <font>
                        <Font name="Book Antiqua" size="36.0"/>
                    </font>
                </PasswordField>
            </HBox>
            <HBox alignment="CENTER" prefHeight="64.0" prefWidth="264.0">
                <Button maxWidth="-Infinity" mnemonicParsing="false" onAction="#onClickButtonOK" prefWidth="300.0"
                        style="-fx-background-color: #006644;" text="ОК" textFill="WHITE">
                    <font>
                        <Font name="Book Antiqua" size="36.0"/>
                    </font>
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                </Button>
                <Button maxWidth="-Infinity" mnemonicParsing="false" onAction="#onClickButtonRegistration"
                        prefWidth="300.0" style="-fx-background-color: #ffffff; -fx-border-style: solid;"
                        text="Регистрация">
                    <font>
                        <Font name="Book Antiqua" size="36.0"/>
                    </font>
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                </Button>
            </HBox>
        </VBox>
    </AnchorPane>
</AnchorPane>
